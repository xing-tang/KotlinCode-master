        android:text="Hello Boolean!" />

    <TextView
        android:id="@+id/mDoubleTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Hello Double!" />

    <TextView
        android:id="@+id/mFloatTv"
        android:layout_width="wrap_content"
